@model LearnSchoolApp.Entities.Guide

<div>
    <h4>שלום @Model.name</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            מס' סידורי
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class="col-sm-2">
            ת"ז
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.userId)
        </dd>
        <dt class="col-sm-2">
            שם
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.name)
        </dd>
        <dt class="col-sm-2">
            שם משתמש
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.username)
        </dd>
        <dt class="col-sm-2">
            סיסמה
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.password)
        </dd>
        <dt class="col-sm-2">
            מייל
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.email)
        </dd>
        <dt class="col-sm-2">
            מס' טלפון
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.phone)
        </dd>
        <dt class="col-sm-2">
            פרויקט
        </dt>
        <dd class="col-sm-10">
            @Html.ActionLink("פרטים", "MyProjects", new { id = Model.userId })
        </dd>
    </dl>
</div>

