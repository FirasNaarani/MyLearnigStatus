@model LearnSchoolApp.Entities.Project

<h4>פרויקט</h4>
<h3>?האם אתה בטוח שברצונך למחוק את זה</h3>
<div>
    <div>
        @Html.ActionLink("ביטול", "Details", new { id = Model.Id })
    </div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            מס' סידורי
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class="col-sm-2">
            שם
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.name)
        </dd>
        <dt class="col-sm-2">
            ת"ז המנחה
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.guideId)
        </dd>
        <dt class="col-sm-2">
            שם המנחה
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.guideName)
        </dd>
        <dt class="col-sm-2">
            ת"ז סטודנט ראשי
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.studentId)
        </dd>
        <dt class="col-sm-2">
            ת"ז סטודנט שותף
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.assistantStudentId)
        </dd>
        <dt class="col-sm-2">
            מאושר
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.isPass)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="submit" value="מחיקה" class="btn btn-danger" />
    </form>
</div>
